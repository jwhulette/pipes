{
    "name": "jwhulette/pipes",
    "description": "An PHP ETL Processing Library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Wes Hulette",
            "email": "jwhulette@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4",
        "league/pipeline": "dev-master",
        "box/spout": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "jwhulette\\pipes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always",
        "phpstan": "phpstan analyse",
        "test-all": [
            "phpstan analyse",
            "phpunit --colors=always"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
            ]
        }
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.26.2",
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^8.5",
        "phpstan/phpstan": "^0.12.3",
        "ramsey/uuid": "^3.9",
        "orchestra/testbench": "^4.4"
    }
}
