{
    "name": "jwhulette/pipes",
    "description": "An PHP ETL Processing Library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Wes Hulette",
            "email": "jwhulette@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0|^8.1",
        "league/pipeline": "^1.0",
        "box/spout": "^3.1",
        "spatie/laravel-package-tools": "^1.9",
        "league/csv": "^9.0",
        "spatie/data-transfer-object": "^3.0",
        "illuminate/container": "^8.0|^9.0",
        "illuminate/database": "^8.0|^9.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.18|^3.0",
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^0.12|^1.5",
        "ramsey/uuid": "^4.1",
        "orchestra/testbench": "^6.0|^7.0",
        "mikey179/vfsstream": "^1.6",
        "nunomaduro/larastan": "^0.7|^2.0",
        "spatie/temporary-directory": "^2.0",
        "nunomaduro/phpinsights": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "Jwhulette\\Pipes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always",
        "phpstan": "vendor/bin/phpstan analyse --memory-limit=2G",
        "tests": [
            "vendor/bin/phpstan analyse --memory-limit=2G",
            "phpunit --colors=always"
        ],
        "insights": "./vendor/bin/phpinsights --no-interaction --min-quality=90 --min-complexity=80 --min-architecture=90 --min-style=95n"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jwhulette\\Pipes\\PipesServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
