{
    "name": "jwhulette/pipes",
    "description": "An PHP ETL Processing Library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Wes Hulette",
            "email": "jwhulette@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8",
        "league/pipeline": "dev-master",
        "box/spout": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "jwhulette\\pipes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always",
        "phpstan": "phpstan analyse",
        "ts": [
            "phpstan analyse",
            "phpunit --colors=always"
        ]    
    },
    "extra": {
        "laravel": {
            "providers": [
                "jwhulette\\Pipes\\PipesServiceProvider"
            ]
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.18",
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^0.12.73",
        "ramsey/uuid": "^4.1",
        "orchestra/testbench": "^6.11",
        "codedungeon/phpunit-result-printer": "^0.30.1",
        "mikey179/vfsstream": "^1.6",
        "nunomaduro/larastan": "^0.7.0"
    }
}
