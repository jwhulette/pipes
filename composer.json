{
    "name": "jwhulette/pipes",
    "description": "An ETL'ish Processing Library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Wes Hulette",
            "email": "jwhulette@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1",
        "league/pipeline": "dev-master",
        "box/spout": "^3.1@dev"
    },
    "autoload": {
        "psr-4": {
            "jwhulette\\pipes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jwhulette\\Pipes\\PipesServiceProvider"
            ]
        }
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "dev-master",
        "friendsofphp/php-cs-fixer": "^2.16@dev",
        "phpunit/phpunit": "^8.4@dev",
        "phpstan/phpstan": "^0.12.0@dev",
        "ramsey/uuid": "^3.0",
        "orchestra/testbench": "^4.0"
    }
}
